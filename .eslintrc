{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "plugin:react/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "semi": [
      "warn",
      "always"
    ],
    "quotes": [
      "warn",
      "single"
    ],
    "react/state-in-constructor": 0,
    "react/prop-types": "off",
    "no-unused-vars": "warn",
    "no-underscore-dangle": 0,
    "no-trailing-spaces": [
      "warn",
      {
        "ignoreComments": true
      }
    ],
    "linebreak-style": 0,
    "func-names": [
      "warn",
      "never"
    ]
  }
}